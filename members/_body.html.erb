<table class="attr">
 <tr>
   <th width="150">年齢</th>
   <td><%= @member.age %></td>
 </tr>
 <tr>
    <th>ユーザー名</th>
    <td><%= @member.name %></td>
 </tr>
 <tr>
 <th>氏名</th>
 <td><%= @member.full_name %></td>
 </tr>
